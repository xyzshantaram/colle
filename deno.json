    "fmt": {
        "indentWidth": 4,
        "lineWidth": 100
    },
    "tasks": {
        "test": "deno test --allow-env=LOCAL,DENO_DIR,XDG_CACHE_HOME,HOME --allow-write=/tmp --allow-read --allow-ffi --allow-net --unstable-kv api.test.ts",
